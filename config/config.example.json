{
    "deepseek_api": {
        "api_key": "YOUR_DEEPSEEK_API_KEY",
        "model": "deepseek-coder-v2",
        "embedding_model": "deepseek-embedding",
        "temperature": 0.7,
        "max_tokens": 4000,
        "base_url": "https://api.deepseek.com",
        "dev_mode": true
    },
    "paths": {
        "templates_dir": "./templates",
        "datasets_dir": "./data/adam",
        "output_dir": "./output",
        "cache_dir": "./data/cache",
        "vector_db_dir": "./data/vector_db",
        "sessions_dir": "./data/sessions",
        "logs_dir": "./logs"
    },
    "rag": {
        "similarity_threshold": 0.6,
        "max_templates": 3,
        "use_vector_search": true,
        "embedding_dimension": 128
    },
    "r_integration": {
        "packages": [
            "tidyverse",
            "haven",
            "gt",
            "flextable",
            "officer",
            "r2rtf"
        ],
        "debug_mode": true,
        "auto_fix": true,
        "max_fix_attempts": 3,
        "timeout": 60
    },
    "ui": {
        "theme": "default",
        "port": 7860,
        "share": false,
        "debug": false,
        "default_interface": "workflow"
    },
    "assistant": {
        "model": "deepseek-chat",
        "temperature": 0.7,
        "max_history_length": 10,
        "auto_save_interval": 60
    },
    "session": {
        "auto_save": true,
        "auto_save_interval": 60,
        "max_sessions": 20
    }
} 